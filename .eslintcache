obal';\n\ninterface UserData {\n  email: string;\n  password: string;\n}\n\nfunction SignIn(): JSX.Element {\n  const { signIn } = useAuth();\n  const history = useHistory();\n  const { addPopUp } = usePopUp();\n\n  const { handleSubmit, register, errors } = useForm<UserData>({\n    resolver: yupResolver(schema),\n  });\n\n  const onSubmit = React.useCallback(\n    async (values: UserData) => {\n      try {\n        await signIn({ email: values.email, password: values.password });\n        addPopUp({ icon: 'Succes', title: 'Succes', description: 'Has Login' });\n        history.push('/');\n      } catch (err) {\n        console.log(err);\n        addPopUp({\n          icon: 'Error',\n          title: `Error`,\n          description: 'not logged in',\n        });\n      }\n    },\n    [signIn, history, addPopUp],\n  );\n\n  return (\n    <>\n      <Container>\n        <Content>\n          <img src={Logo} alt=\"logo\" />\n          <form onSubmit={handleSubmit(onSubmit)}>\n            <h1>sign in</h1>\n            <Input\n              className={errors.email ? 'borderError' : ''}\n              icon=\"Email\"\n              name=\"email\"\n              type=\"text\"\n              placeholder=\"email\"\n              register={register}\n            />\n            <Input\n              className={errors.password ? 'borderError' : ''}\n              icon=\"Password\"\n              name=\"password\"\n              type=\"text\"\n              placeholder=\"password\"\n              register={register}\n            />\n            <Button type=\"submit\">login</Button>\n            <a href=\"forgot\">you have forgotten the password?</a>\n          </form>\n          <Link to=\"register\">\n            <FiLogIn />\n            register\n          </Link>\n        </Content>\n\n        <Background />\n      </Container>\n      <GlobalStyle />\n    </>\n  );\n}\n\nexport default SignIn;\n","/home/isaias/Desktop/bootcamp/goBarber/src/services/api.ts",[],"/home/isaias/Des